{
    "manifest_version": 2,
    "name": "Advance__Hello_NewTab",
    "version": "2.0.0",
    "description": "The bookmark list on the new tab page.",
    "permissions": [
        "bookmarks",
        "tabs",
        "storage",
        "chrome://favicon/",
        "notifications"
    ],
    "icons": {
        "16": "img/icon_016.png",
        "48": "img/icon_048.png",
        "128": "img/icon_128.png"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "options_page": "option.html"
}
