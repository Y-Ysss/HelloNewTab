{
    "manifest_version": 2,
    "name": "Hello New Tab",
    "version": "3.1.5",
    "description": "Startpage giving instant access to bookmarks.",
    "permissions": [
        "bookmarks",
        "tabs",
        "storage",
        "chrome://favicon/",
        "notifications"
    ],
    "icons": {
        "16": "img/icon_016.png",
        "48": "img/icon_048.png",
        "128": "img/icon_128.png"
    },
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "options_page": "option.html"
}
